{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": false
    },
    "buildOnSave": false,
    "filesGlob": [
        "src/**/*.ts",
        "test/**/*.ts",
        "!node_modules/**/*.*",
        "!bower_components/**/*.*"
    ],
    "files": [
        "src/_typedefs/data/Property.d.ts",
        "src/build.r.ts",
        "src/collection/ArrayBase.ts",
        "src/collection/ArraySet.ts",
        "src/collection/collection.ts",
        "src/collection/EntitySet.ts",
        "src/collection/SimpleMap.ts",
        "src/collection/SortedArraySet.ts",
        "src/config.amd.ts",
        "src/config.ts",
        "src/data/Association.ts",
        "src/data/data.ts",
        "src/data/decorators.ts",
        "src/data/Entity.ts",
        "src/data/EntityManager.ts",
        "src/data/Field.ts",
        "src/data/IContext.ts",
        "src/data/Identity.ts",
        "src/data/IObjectChangeRecord.d.ts",
        "src/data/IStore.d.ts",
        "src/data/IStoreFactory.d.ts",
        "src/data/Metadata.ts",
        "src/data/Model.ts",
        "src/data/ObjectMapper.ts",
        "src/data/Observable.ts",
        "src/data/StoreFactory.d.ts",
        "src/Destroyable.ts",
        "src/di/Inject.ts",
        "src/lang/typescript.ts",
        "src/main.ts",
        "src/util/defaults.ts",
        "src/util/Log.ts",
        "src/util/type.ts",
        "src/util/util.ts",
        "test/unit/collection/ArrayBase.ts",
        "test/unit/collection/ArrayCommon.ts",
        "test/unit/collection/ArraySet.ts",
        "test/unit/collection/EntitySet.ts",
        "test/unit/collection/SortedArraySet.ts",
        "test/unit/data/Association.ts",
        "test/unit/data/decorators.ts",
        "test/unit/data/EntityManager.ts",
        "test/unit/data/Model.ts",
        "test/unit/data/Property.ts",
        "test/unit/guide/modeling-data.ts",
        "test/unit/mocks/MemoryStore.ts",
        "test/unit/mocks/mocks.ts",
        "test/unit/mocks/StoreFactory.ts",
        "test/unit/util/defaults.ts",
        "test/unit/util/type.ts"
    ],
    "exclude": [
        "node_modules/**/*.*",
        "bower_components/**/*.*"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
